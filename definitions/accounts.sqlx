config {
  type: "table",
  bigquery: {
    partitionBy: "DATE(ts)"
  }
}

SELECT 
  CURRENT_TIMESTAMP() AS TS, 
  100 AS AccountNumber, 
  'ricardo' AS ClientName,
  'HomeLoan' AS AccountType
UNION ALL
SELECT 
  CURRENT_TIMESTAMP() AS TS, 
  101 AS AccountNumber, 
  'polito' AS ClientName,
  'HomeLoan' AS AccountType
UNION ALL
SELECT 
  CURRENT_TIMESTAMP() AS TS, 
  102 AS AccountNumber, 
  'julis' AS ClientName,
  'CarLoan' AS AccountType
